{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:allow-execute",
    "shell:allow-kill",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-exists",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "stronghold:allow-save",
    "stronghold:allow-load"
  ]
}
